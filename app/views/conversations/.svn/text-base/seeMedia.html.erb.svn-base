
<div id="contentList" class="cf">

<%= link_to_function "<h5>Fotografías</h5>" do |page| page.visual_effect(:toggle_blind, 'photoList', :duration => 0.5) end %> 
<div id="photoList" class="cf" style="display:none;">
	<% for img in @images %>
	<ul>
		<a href="#">
			<li class="photoThumbnail">
				<div class="clipout">
				    <div class="clipin">
				    	<%= image_tag img.url, :alt => "fotografía" %>
				   	</div>
			   	</div>
			   	<p><a class="moreInfoIcon" href="#">Detalles</a></p>
			</li>
		</a>
	</ul>
	<% end %>
	
</div>

