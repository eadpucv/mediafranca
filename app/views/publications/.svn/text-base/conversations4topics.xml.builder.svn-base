xml.instruct!
xml.conversations do
	for c in @conv
		xml.conversation do
		      xml.id c.id
		      xml.title c.title
		      xml.tags_coincidentes c.tags_coincidentes
		      xml.tags c.tags
		      xml.total_publicaciones c.total_publicaciones
		      xml.ultima_publicacion c.ultima_publicacion
		      xml.relevance c.relevance
		end
	end
end